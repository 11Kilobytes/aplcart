:Class index : MiPage
    ∇ Compose
      :Access public
      :Trap 0
          table←⎕CSV⍠'Separator'(⎕UCS 9)⍠'QuoteChar' ''⍠'IfExists' 'Replace'⊂#.Boot.AppRoot,'table.tsv'
          {⎕SIGNAL ⍵}(9≠cols)/⊂('EN' 5)('Message'('Table has ',(⍕cols←⊃⌽⍴table),' instead of 9 columns'))
          Add _.Redirect'/index.html'
      :Else
          Add _.h1(⎕←⎕DMX.Message)
      :EndTrap
    ∇
:EndClass
